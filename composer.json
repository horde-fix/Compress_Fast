{
    "minimum-stability": "dev",
    "name": "horde/compress_fast",
    "description": "Fast compression library",
    "type": "library",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "time": "2021-11-06",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "ext-zlib": "*",
        "ext-lzf": "*",
        "ext-horde_lz4": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Compress_Fast": "lib/"
        },
        "psr-4": {
            "Horde\\Compress\\Fast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Compress\\Fast\\Test\\": "test/"
        }
    }
}