 <dependencies>
  <required>
   <php>
    <min>7.0.0</min>
    <max>8.0.0alpha1</max>
    <exclude>8.0.0alpha1</exclude>
   </php>
   <pearinstaller>
    <min>1.7.0</min>
   </pearinstaller>
  </required>
  <optional>
   <extension>
    <name>zlib</name>
   </extension>
   <extension>
    <name>lzf</name>
   </extension>
   <extension>
    <name>horde_lz4</name>
   </extension>
  </optional>
 </dependencies>
